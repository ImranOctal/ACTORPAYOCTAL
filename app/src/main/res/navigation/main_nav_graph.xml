    <fragment
        android:id="@+id/promoListFragment"
        android:name="com.octal.actorpay.ui.promocodes.PromoListFragment"
        android:label="PromoListFragment" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.octal.actorpay.ui.settings.SettingsFragment"
        android:label="SettingsFragment" />
    <fragment
        android:id="@+id/shippingAddressFragment"
        android:name="com.octal.actorpay.ui.shippingaddress.ShippingAddressFragment"
        android:label="ShippingAddressFragment" />
    <fragment
        android:id="@+id/orderDetailsFragment"
        android:name="com.octal.actorpay.ui.myOrderList.orderdetails.OrderDetailsFragment"
        android:label="OrderDetailsFragment" />
    <fragment
        android:id="@+id/addMoneyFragment"
        android:name="com.octal.actorpay.ui.addmoney.AddMoneyFragment"
        android:label="AddMoneyFragment" />

</navigation>