<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav_graph"
    app:startDestination="@id/homeBottomFragment">



    <fragment
        android:id="@+id/homeBottomFragment"
        android:name="com.octal.actorpayuser.ui.dashboard.bottomnavfragments.home.HomeBottomFragment"
        android:label="@string/home"
        tools:layout="@layout/fragment_home_bottom">
        <action
            android:id="@+id/action_homeBottomFragment_to_walletBottomFragment2"
            app:destination="@id/walletBottomFragment" />
        <action
            android:id="@+id/action_homeBottomFragment_to_walletDetailsFragment"
            app:destination="@id/walletDetailsFragment" />


    </fragment>
    <fragment
        android:id="@+id/profileBottomFragment"
        android:name="com.octal.actorpayuser.ui.dashboard.bottomnavfragments.ProfileBottomFragment"
        android:label="@string/my_profile"
        tools:layout="@layout/fragment_profile_bottom">

    </fragment>
    <fragment
        android:id="@+id/historyBottomFragment"
        android:name="com.octal.actorpayuser.ui.dashboard.bottomnavfragments.HistoryBottomFragment"
        android:label="@string/history"
        tools:layout="@layout/fragment_history_bottom">

    </fragment>



    <fragment
        android:id="@+id/walletFragment"
        android:name="com.octal.actorpayuser.ui.walletStatement.WalletStatementFragment"
        android:label="@string/wallet_statement"
        tools:layout="@layout/fragment_walletw_statement">


        <action
            android:id="@+id/action_walletFragment_to_homeFragment"
            app:destination="@+id/homeFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="true" />
    </fragment>

    <fragment
        android:id="@+id/transferMoneyFragment"
        android:name="com.octal.actorpayuser.ui.transferMoney.TransferMoneyFragment"
        android:label="@string/transfer_money"
        tools:layout="@layout/fragment_transfer_money">
        <!-- <action
             app:popUpTo="@id/homeFragment"
             app:popUpToInclusive="true" />-->
    </fragment>

    <fragment
        android:id="@+id/productListFragment"
        android:name="com.octal.actorpayuser.ui.productList.ProductsListFragment"
        android:label="@string/product_list"
        tools:layout="@layout/fragment_products_list" />

    <fragment
        android:id="@+id/notificationFragment"
        android:name="com.octal.actorpayuser.ui.notification.NotificationFragment"
        android:label="@string/notification"
        tools:layout="@layout/fragment_notification" />
    <fragment
        android:id="@+id/referFragment"
        android:name="com.octal.actorpayuser.ui.refer_and_earn.ReferAndEarnFragment"
        android:label="@string/referral"
        tools:layout="@layout/fragment_refer_and_earn" />


    <fragment
        android:id="@+id/myOrderFragment"
        android:name="com.octal.actorpayuser.ui.myOrderList.MyOrdersListFragment"
        android:label="@string/my_order"
        tools:layout="@layout/fragment_my_order_list" />
    <fragment
        android:id="@+id/rewardsFragment"
        android:name="com.octal.actorpayuser.ui.rewards_points.RewardsPointsFragment"
        android:label="@string/rewards"
        tools:layout="@layout/fragment_rewards_points" />

    <fragment
        android:id="@+id/miscFragment"
        android:name="com.octal.actorpayuser.ui.misc.MiscFragment"
        android:label="@string/misc"
        tools:layout="@layout/fragment_misc" />

    <fragment
        android:id="@+id/remittance"
        android:name="com.octal.actorpayuser.ui.remittance.RemittanceFragment"
        android:label="@string/remittance"
        tools:layout="@layout/fragment_remittance"/>
    <fragment
        android:id="@+id/faqFragment"
        android:name="com.octal.actorpayuser.ui.misc.FAQFragment"
        android:label="fragment_f_a_q"
        tools:layout="@layout/fragment_f_a_q" />
    <fragment
        android:id="@+id/walletBottomFragment"
        android:name="com.octal.actorpayuser.ui.dashboard.bottomnavfragments.wallet.WalletBottomFragment"
        android:label="WalletBottomFragment" />
    <fragment
        android:id="@+id/promoListFragment"
        android:name="com.octal.actorpayuser.ui.promocodes.PromoListFragment"
        android:label="PromoListFragment" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.octal.actorpayuser.ui.settings.SettingsFragment"
        android:label="SettingsFragment" />
    <fragment
        android:id="@+id/shippingAddressFragment"
        android:name="com.octal.actorpayuser.ui.shippingaddress.ShippingAddressFragment"
        android:label="ShippingAddressFragment" />
    <fragment
        android:id="@+id/orderDetailsFragment"
        android:name="com.octal.actorpayuser.ui.myOrderList.orderdetails.OrderDetailsFragment"
        android:label="OrderDetailsFragment" >
        <action
            android:id="@+id/action_orderDetailsFragment_to_disputeFragment"
            app:destination="@id/disputeFragment"
            app:popUpTo="@id/homeBottomFragment"
            app:popUpToInclusive="false"
            />
    </fragment>
    <fragment
        android:id="@+id/addMoneyFragment"
        android:name="com.octal.actorpayuser.ui.addmoney.AddMoneyFragment"
        android:label="AddMoneyFragment" />
    <fragment
        android:id="@+id/payFragment"
        android:name="com.octal.actorpayuser.ui.transferMoney.PayFragment"
        android:label="PayFragment" />
    <fragment
        android:id="@+id/mobileAndDTHFragment"
        android:name="com.octal.actorpayuser.ui.mobileanddth.MobileAndDTHFragment"
        android:label="MobileAndDTHFragment" />
    <fragment
        android:id="@+id/disputeFragment"
        android:name="com.octal.actorpayuser.ui.dispute.DisputeFragment"
        android:label="DisputerFragment" />
    <fragment
        android:id="@+id/disputeDetailsFragment"
        android:name="com.octal.actorpayuser.ui.dispute.disputedetails.DisputeDetailsFragment"
        android:label="DisputeDetailsFragment" />
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.octal.actorpayuser.ui.cart.CartFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_cart" >
        <action
            android:id="@+id/action_cartFragment_to_productListFragment"
            app:destination="@id/productListFragment"
            app:popUpTo="@id/homeBottomFragment"
            app:popUpToInclusive="false"
            />
    </fragment>
    <fragment
        android:id="@+id/productDetailsFragment"
        android:name="com.octal.actorpayuser.ui.productList.productdetails.ProductDetailsFragment"
        android:label="fragment_product_details"
        tools:layout="@layout/fragment_product_details" />
    <fragment
        android:id="@+id/placeOrderFragment"
        android:name="com.octal.actorpayuser.ui.myOrderList.placeorder.PlaceOrderFragment"
        android:label="fragment_place_order"
        tools:layout="@layout/fragment_place_order" />
    <fragment
        android:id="@+id/walletDetailsFragment"
        android:name="com.octal.actorpayuser.ui.dashboard.bottomnavfragments.wallet.WalletDetailsFragment"
        android:label="WalletDetailsFragment" />
    <fragment
        android:id="@+id/walletUserFragment"
        android:name="com.octal.actorpayuser.ui.dashboard.bottomnavfragments.wallet.walletuser.WalletUserFragment"
        android:label="WalletUserFragment" >
        <action
            android:id="@+id/action_walletUserFragment_to_walletDetailsFragment"
            app:destination="@id/walletDetailsFragment"
            app:popUpTo="@id/walletDetailsFragment"
            app:popUpToInclusive="true"
            />
    </fragment>
    <fragment
        android:id="@+id/requestMoneyFragment"
        android:name="com.octal.actorpayuser.ui.request_money.RequestMoneyFragment"
        android:label="RequestMoneyFragment" />
    <fragment
        android:id="@+id/receiveFragment"
        android:name="com.octal.actorpayuser.ui.request_money.ReceiveFragment"
        android:label="fragment_receive"
        tools:layout="@layout/fragment_receive" />
    <fragment
        android:id="@+id/requestMoneyDetailsFragment"
        android:name="com.octal.actorpayuser.ui.request_money.RequestMoneyDetailsFragment"
        android:label="RequestMoneyDetailsFragment" />

</navigation>